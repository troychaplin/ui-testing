{
  "name": "@carletonuniversity/rds",
  "private": false,
  "version": "0.7.2",
  "license": "MIT",
  "type": "module",
  "main": "./dist/rds.umd.js",
  "module": "./dist/rds.es.js",
  "types": "./dist/index.d.ts",
  "files": ["/dist", "/dist/index.css"],
  "exports": {
    ".": {
      "import": "./dist/rds.es.js",
      "require": "./dist/rds.umd.js",
      "style": "./dist/index.css",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "concurrently 'yarn dev:css' 'yarn icon-list' 'tsc && vite build --watch'",
    "dev:css": "tailwindcss -w -i ./src/styles/index.css -o ./public/index.css",
    "build": "tsc && yarn format && yarn lint && yarn icon-list && vite build && yarn build:css && yarn size",
    "build:css": "tailwindcss -m -i ./src/styles/index.css -o ./dist/index.css",
    "storybook": "concurrently 'yarn storybook:css' 'storybook dev -p 6006'",
    "storybook:css": "tailwindcss -w -i ./src/styles/index.css -o ./.storybook/index.css",
    "build-storybook": "concurrently 'yarn build-storybook:css' 'storybook build'",
    "build-storybook:css": "tailwindcss -m -i ./src/styles/index.css -o ./.storybook/index.css",
    "icon-list": "node ./generateIconList.js",
    "size": "size-limit",
    "format": "prettier -w src",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "clean": "rm -rf node_modules ; yarn install",
    "rebuild": "rm -rf node_modules ; rm yarn.lock ; yarn install"
  },
  "resolutions": {
    "prettier": "^2.8.1",
    "strip-ansi": "^6.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "2.1.1",
    "ci": "^2.3.0",
    "priority-plus": "1.5.1",
    "rds-tailwind-theme": "0.6.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-intersection-observer": "9.5.3",
    "tailwindcss": "3.4.1"
  },
  "peerDependencies": {
    "@react-google-maps/api": "2.19.2",
    "date-fns": "3.3.1",
    "formik": "2.4.5",
    "react-places-autocomplete": "7.3.0",
    "react-player": "2.14.1",
    "react-quill": "2.0.0",
    "yup": "1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.4.4",
    "@react-google-maps/api": "2.19.2",
    "@rollup/plugin-terser": "0.4.4",
    "@size-limit/esbuild": "11.0.2",
    "@size-limit/esbuild-why": "11.0.2",
    "@size-limit/file": "11.0.2",
    "@storybook/addon-a11y": "7.6.8",
    "@storybook/addon-docs": "7.6.14",
    "@storybook/addon-essentials": "7.6.8",
    "@storybook/addon-interactions": "7.6.8",
    "@storybook/addon-links": "7.6.8",
    "@storybook/blocks": "7.6.8",
    "@storybook/react": "7.6.8",
    "@storybook/react-vite": "7.6.8",
    "@storybook/testing-library": "0.2.2",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/react-places-autocomplete": "7.2.14",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-react": "4.2.1",
    "@whitespace/storybook-addon-html": "5.1.6",
    "autoprefixer": "10.4.16",
    "concurrently": "8.2.2",
    "copyfiles": "2.4.1",
    "date-fns": "3.3.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-storybook": "0.6.15",
    "formik": "2.4.5",
    "husky": "8.0.3",
    "postcss": "8.4.33",
    "postcss-import": "16.0.0",
    "prettier": "3.2.2",
    "prettier-plugin-tailwindcss": "0.5.11",
    "prop-types": "15.8.1",
    "react-places-autocomplete": "7.3.0",
    "react-player": "2.14.1",
    "react-quill": "2.0.0",
    "react-syntax-highlighter": "15.5.0",
    "size-limit": "11.0.2",
    "storybook": "7.6.8",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-dts": "3.7.1",
    "vite-tsconfig-paths": "4.3.1",
    "yup": "1.3.3"
  }
}
